<%= turbo_frame_tag transaction do %>
  <tr>
    <td><%= transaction.title %></td>
    <td><%= transaction.amount %></td>
    <td><%= transaction.type %></td>
    <td><%= transaction.date %></td>
    <td><%= transaction.category_name %></td>
    <td><%= transaction.group_name %></td>
  </tr>
<% end %>
